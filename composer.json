{
    "name": "forseti/sade-rlseguros",
    "description": "Reservas automatizadas dos portais BB, CN e IO para RL Seguros.",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.forseti.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "forseti/bot-bb": "^1.1",
        "forseti/bot-cn": "^1.5",
        "forseti/bot-io": "^1.0",
        "forseti/bot-sade": "dev-master",
        "forseti/norecaptcha": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-notification-channels/telegram": "^0.0.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.6.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
            "app/sade-helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
